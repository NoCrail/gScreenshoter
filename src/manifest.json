{

    "manifest_version": 2,
    "name": "gScreenshoter",
    "version": "0.6",
  
    "description": "Grafana screenshot",
    "icons": {
      "64": "icons/icon64.png"
    },
  
    "applications": {
      "gecko": {
        "id": "vlad-s-d@ya.ru",
        "strict_min_version": "45.0"
      }
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
        "default_icon": "icons/icon64.png",
        "default_title": "gScreenshoter",
        "default_popup": "popup/popup.html"
      },
    "content_scripts": [
      {
        "matches": ["<all_urls>"]
        
      }
    ]
  
  }